{
  "name": "eslint-config-cmp-node",
  "version": "0.0.11",
  "description": "Eslint config for cmp nodejs project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "test": "LOG_LEVEL=silent jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AkshayKomarla/eslint-config-cmp-node"
  },
  "keywords": [
    "javascript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier-eslint": "^9.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "prettier": "^1.16.4",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  }
}